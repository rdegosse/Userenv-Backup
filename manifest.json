{
 "package": "Userenv-Backup",
 "language": "python",
 "author": "rdegosse",
 "description": "Backup userenv in tool meta data",
 "version": "1.0.0",
 "min_os_version_major": 6,
 "url": "https://raw.githubusercontent.com/rdegosse/Userenv-Backup/master/manifest.json",
